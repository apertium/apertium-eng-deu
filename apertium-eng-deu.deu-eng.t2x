			</pattern>
			<action>
				<out>
					<chunk>
						<lit v="default"/>
						<lit-tag v="default"/>
						<lit v="{^"/>
						<get-case-from pos="1">
							<lit v="more"/>
						</get-case-from>
						<lit-tag v="adv"/>
						<lit v="$}"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


