	<section-def-attrs>
		<def-attr n="nbr">
			<attr-item tags="sg"/>
			<attr-item tags="pl"/>
			<attr-item tags="sp"/>
			<attr-item tags="ND"/>
		</def-attr>
	</section-def-attrs>


	<section-def-vars>
		<def-var n="v_nombre"/>
		<def-var n="v_genere"/>
		<def-var n="v_numero"/>
	</section-def-vars>

	
	<section-def-macros>

		<def-macro n="nombre_SN" npar="1">
			<!-- To determine value of variables of gender and number and propagate
				to other rules. Variables are not used again in this rule -->
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="nbr"/>
							<lit-tag v="sg"/>
						</equal>
					</test>
					<let><var n="v_nombre"/><lit-tag v="sg"/></let>
				</when>
				<when>
					<test>
						<equal>
							<clip pos="1" part="nbr"/>
							<lit-tag v="pl"/>
						</equal>
					</test>
					<let><var n="v_nombre"/><lit-tag v="pl"/></let>
				</when>
				<otherwise>
					<let><var n="v_nombre"/><lit-tag v="sg"/></let>
				</otherwise>
			</choose>
		</def-macro>

	</section-def-macros>


	<section-rules>

		<rule comment="REGLA: N (set ND to sg)">
			<pattern>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<choose>
					<when>
						<test>
							<equal>
								<clip pos="1" part="nbr"/>
								<lit-tag v="ND"/>
							</equal>
						</test>
						<let>
							<clip pos="1" part="nbr"/>
							<lit-tag v="sg"/>
						</let>
					</when>
				</choose>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
				</out>
				<call-macro n="nombre_SN">
					<with-param pos="1"/>
				</call-macro>
			</action>
		</rule>


		<!-- insert "more" -->
		<rule comment="REGLA: MORE ADJ">
			<pattern>
