		</def-cat>
		<!-- <def-cat n="CNJ_ADV">
			<cat-item tags="ADV"/>
			<cat-item tags="ADV.*"/>
			<cat-item tags="CNJ"/>
		</def-cat> -->
		<def-cat n="CM_ADV">
			<cat-item tags="CM_ADV"/>
		</def-cat>
		<def-cat n="PR">
			<cat-item tags="PR"/>
		</def-cat>
		<def-cat n="ATP">
			<cat-item tags="PR.atp"/>
			<cat-item tags="PR.atp.*"/>
		</def-cat>
		<def-cat n="CM">
			<cat-item tags="CM"/>
		</def-cat>
		<def-cat n="SENT">
			<cat-item tags="SENT"/>
			<cat-item tags="QST"/>
