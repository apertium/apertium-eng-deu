<?xml version="1.0" encoding="UTF-8"?> <!-- -*- nxml -*- -->
<interchunk>

	<section-def-cats>

		<def-cat n="SN">
			<cat-item tags="SN"/>
			<cat-item tags="SN.*"/>
		</def-cat>
		<def-cat n="SN_adj">
			<cat-item tags="SN"/>
			<cat-item tags="SN.*"/>
			<cat-item tags="ADJ"/>
			<cat-item tags="ADJ.*"/>
		</def-cat>
		<def-cat n="SNP">
			<cat-item tags="SN"/>
			<cat-item tags="SN.*"/>
			<cat-item tags="PRN"/>
			<cat-item tags="PRN.*"/>
			<cat-item tags="PRPERS"/>
			<cat-item tags="PRPERS.*"/>
		</def-cat>
		<def-cat n="PRN">
			<cat-item tags="PRN"/>
			<cat-item tags="PRN.*"/>
		</def-cat>
		<def-cat n="PRPERS">
			<cat-item tags="PRPERS"/>
			<cat-item tags="PRPERS.*"/>
		</def-cat>
		<def-cat n="PRON">
			<cat-item tags="PRN"/>
			<cat-item tags="PRN.*"/>
			<cat-item tags="PRPERS"/>
			<cat-item tags="PRPERS.*"/>
		</def-cat>
		<def-cat n="ITG">
			<cat-item tags="ITG"/>
		</def-cat>
		<def-cat n="REL">
			<cat-item tags="REL"/>
			<cat-item tags="REL.*"/>
		</def-cat>

		<def-cat n="SV">  <!-- any verb phrase -->
			<cat-item tags="SV"/>
			<cat-item tags="SV.*"/>
			<cat-item tags="SV_aux"/>
			<cat-item tags="SV_aux.*"/>
			<cat-item tags="SV_ser"/>
			<cat-item tags="SV_ser.*"/>
			<cat-item tags="SV_haver"/>
			<cat-item tags="SV_haver.*"/>
			<cat-item tags="SV_do"/>
			<cat-item tags="SV_do.*"/>
		</def-cat>
		<def-cat n="SV_aux">
			<cat-item tags="SV_aux"/>
			<cat-item tags="SV_aux.*"/>
		</def-cat>
		<def-cat n="SV_ser">
			<cat-item tags="SV_ser"/>
			<cat-item tags="SV_ser.*"/>
		</def-cat>
		<def-cat n="SV_haver">
			<cat-item tags="SV_haver"/>
			<cat-item tags="SV_haver.*"/>
		</def-cat>
		<def-cat n="SV_do">
			<cat-item tags="SV_do"/>
			<cat-item tags="SV_do.*"/>
		</def-cat>
		<def-cat n="VPART">
			<cat-item tags="V.PART"/>
		</def-cat>

		<def-cat n="ADJ">
			<cat-item tags="ADJ"/>
			<cat-item tags="ADJ.*"/>
		</def-cat>
		<def-cat n="CNJ">
			<cat-item tags="CNJ"/>
			<cat-item tags="CNJ.*"/>
		</def-cat>
		<def-cat n="ADV">
			<cat-item tags="ADV"/>
			<cat-item tags="ADV.*"/>
		</def-cat>
		<def-cat n="CM_ADV">
			<cat-item tags="CM_ADV"/>
		</def-cat>
		<def-cat n="PR">
			<cat-item tags="PR"/>
		</def-cat>
		<def-cat n="ATP">
			<cat-item tags="PR.atp"/>
			<cat-item tags="PR.atp.*"/>
		</def-cat>
		<def-cat n="CM">
			<cat-item tags="CM"/>
		</def-cat>
		<def-cat n="SENT">
			<cat-item tags="SENT"/>
			<cat-item tags="QST"/>
		</def-cat>

		<def-cat n="ANY_TAG">
			<cat-item tags="*"/>
		</def-cat>
		<!-- <def-cat n="unknown">
			<cat-item tags="unknown"/>
		</def-cat> -->
	</section-def-cats>

	<section-def-attrs>
		<def-attr n="a_gen">			<!-- Gender -->
			<attr-item tags="m"/>
			<attr-item tags="f"/>
			<attr-item tags="nt"/>
			<attr-item tags="mf"/>
			<attr-item tags="mfn"/>
			<attr-item tags="GD"/>
		</def-attr>
		<def-attr n="a_nbr">			<!-- Number -->
			<attr-item tags="sg"/>
			<attr-item tags="pl"/>
			<attr-item tags="sp"/>
			<attr-item tags="ND"/>
		</def-attr>
		<def-attr n="a_pers">			<!-- Person -->
			<attr-item tags="p1"/>
			<attr-item tags="p2"/>
			<attr-item tags="p3"/>
			<attr-item tags="PD"/>
		</def-attr>
	</section-def-attrs>

	<section-def-vars>
		<def-var n="v_nombre"/>
		<def-var n="v_genere"/>
	</section-def-vars>


	<section-def-macros>

		<!-- (verb) if "PD" get person from next noun -->
		<def-macro n="upd_PD_pers" npar="2">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_pers"/>
							<lit-tag v="PD"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_pers"/>
						<clip pos="2" part="a_pers"/>
					</let>
				</when>
			</choose>
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_pers"/>
							<lit-tag v="PD"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_pers"/>
						<lit-tag v="p3"/>
					</let>
				</when>
			</choose>
		</def-macro>

		<!-- (verb) if "ND" get number from next noun -->
		<def-macro n="upd_ND_nbr" npar="2">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_nbr"/>
							<lit-tag v="ND"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_nbr"/>
						<clip pos="2" part="a_nbr"/>
					</let>
				</when>
			</choose>
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_nbr"/>
							<lit-tag v="ND"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_nbr"/>
						<lit-tag v="pl"/>
						<!--<lit-tag v="sg"/>-->
					</let>
				</when>
			</choose>
		</def-macro>

		<!-- (verb) if "GD" get gender from next word -->
		<def-macro n="upd_GD_gen" npar="2">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_gen"/>
							<lit-tag v="GD"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_gen"/>
						<clip pos="2" part="a_gen"/>
					</let>
				</when>
			</choose>
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_gen"/>
							<lit-tag v="GD"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_gen"/>
						<lit-tag v="nt"/>
						<!--<lit-tag v="mf"/>-->
					</let>
				</when>
			</choose>
		</def-macro>

		<!-- (verb) (to be determined) -->
		<!--(only necessary for rules that start with Verbs)-->
		<def-macro n="upd_PD_ND_GD" npar="2">
			<call-macro n="upd_PD_pers">  <!-- PD to "pers" -->
				<with-param pos="1"/>
				<with-param pos="2"/>    <!-- "pers" from noun -->
			</call-macro>
			<call-macro n="upd_ND_nbr">  <!-- ND to "nbr" -->
				<with-param pos="1"/>
				<with-param pos="2"/>    <!-- "nbr" from noun -->
			</call-macro>
			<call-macro n="upd_GD_gen">  <!-- GD to "gen" -->
				<with-param pos="1"/>
				<with-param pos="2"/>    <!-- "gen" from noun -->
			</call-macro>
		</def-macro>

	</section-def-macros>


	<section-rules>

		<!-- "V ~V PR N" => "V PR N ~V" -->
		<rule comment="RULE: V V PR N">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="SV"/>
				<pattern-item n="PR"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="4"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b pos="3"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "can I have (...)" => "kann ich (...) haben" -->
		<!-- "Vx N ~V N" => "Vx N N ~V" -->
		<rule comment="RULE: Vx NP V N">
			<pattern>
				<pattern-item n="SV_aux"/>
				<pattern-item n="SNP"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b pos="3"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "are you drinking coffee" => "trinkst du (gerade) Kaffee" -->
		<!-- "-Vs N ~V N" => "~V N N" -->
		<rule comment="RULE: Vs NP V N">
			<pattern>
				<pattern-item n="SV_ser"/>
				<pattern-item n="SNP"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<get-case-from pos="1">
							<clip pos="3" part="lem"/>
						</get-case-from>
						<clip pos="1" part="tags"/>  <!-- tense from first verb -->
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "do you give your (...)" => "gibst du deiner (...)" -->
		<!-- "-Vd N ~V N" => "~V N N" -->
		<rule comment="RULE: Vd NP V N">
			<pattern>
				<pattern-item n="SV_do"/>
				<pattern-item n="SNP"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- PD to "pers" -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="1" part="tags"/>  <!-- "do" -->
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "do you never give your (...)" => "gibst du deiner (...) nie" -->
		<!-- "-Vd N ~ADV ~V N" => "~V N N ~ADV" -->
		<rule comment="RULE: Vd NP ADV V N">
			<pattern>
				<pattern-item n="SV_do"/>
				<pattern-item n="SNP"/>
				<pattern-item n="ADV"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- PD to "pers" -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="1" part="tags"/>  <!-- "do" -->
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="5" part="lem"/>
						<clip pos="5" part="tags"/>
						<clip pos="5" part="chcontent"/>
					</chunk>
					<b pos="3"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "can I have (...)" => "kann ich (...) haben" -->
		<!-- "Vx N ~V N N" => "Vx N N N ~V" -->
		<rule comment="RULE: Vx NP V NP N">
			<pattern>
				<pattern-item n="SV_aux"/>
				<pattern-item n="SNP"/>
				<pattern-item n="SV"/>
				<pattern-item n="SNP"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b pos="3"/>
					<chunk>
						<clip pos="5" part="lem"/>
						<clip pos="5" part="tags"/>
						<clip pos="5" part="chcontent"/>
					</chunk>
					<b pos="4"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- separable verb -->
		<!-- "V ~ATP N" => "V N ~ATP" -->
		<rule comment="RULE: V ATP NP">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="ATP"/>
				<pattern-item n="SNP"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="3"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- update nbr/ND -->
		<rule comment="RULE: V PR PRN">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="PR"/>
				<pattern-item n="PRON"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="3"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- (pl): "(he) has just played football" => "(er) hat gerade FuÃŸball gespielt " -->
		<!-- V ADV ~V N => V ADV N ~V --> <!-- update nbr/ND -->
		<rule comment="RULE: V ADV V N">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="ADV"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="4"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b pos="3"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>

		<!-- "V ~N ADV" => "V ADV ~N" -->
		<rule comment="RULE: V N ADV">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
				<pattern-item n="ADV"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- update nbr/ND -->
		<!-- "look at your calendar" => "sieh auf deinen Kalender" -->
		<rule comment="RULE: V N , ADV">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="SN_adj"/>
				<pattern-item n="CM_ADV"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b pos="2"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- update nbr/ND -->  <!--(this rule can block rules that start with SN and have only 2 chunks!)-->
		<!-- "look at your calendar" => "sieh auf deinen Kalender" -->
		<rule comment="RULE: V NP">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="SNP"/>
			</pattern>
			<action>
				<call-macro n="upd_PD_ND_GD">  <!-- (to be determined) -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b pos="1"/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
