	<section-def-attrs>
		<def-attr n="a_gen">
			<attr-item tags="m"/>
			<attr-item tags="f"/>
			<attr-item tags="nt"/>
			<attr-item tags="mf"/>
			<attr-item tags="mfn"/>
			<attr-item tags="GD"/>
		</def-attr>
		<def-attr n="a_nbr">
			<attr-item tags="sg"/>
			<attr-item tags="pl"/>
			<attr-item tags="sp"/>
			<attr-item tags="ND"/>
		</def-attr>
	</section-def-attrs>

	<section-def-vars>
		<def-var n="v_nombre"/>
		<def-var n="v_genere"/>
	</section-def-vars>


	<section-def-macros>

		<!-- (verb) if "ND" get number from next noun -->
		<!--(only necessary for rules that start with Verbs)-->
		<def-macro n="upd_ND_nbr" npar="2">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_nbr"/>
							<lit-tag v="ND"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_nbr"/>
						<clip pos="2" part="a_nbr"/>
					</let>
				</when>
			</choose>
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_nbr"/>
							<lit-tag v="ND"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_nbr"/>
						<lit-tag v="sg"/>
					</let>
				</when>
			</choose>
		</def-macro>

		<!-- (verb) if "GD" get gender from next word -->
		<def-macro n="upd_GD_gen" npar="2">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" part="a_gen"/>
							<lit-tag v="GD"/>
						</equal>
					</test>
					<let>
						<clip pos="1" part="a_gen"/>
						<clip pos="2" part="a_gen"/>
					</let>
				</when>
			</choose>
		</def-macro>

	</section-def-macros>


	<section-rules>

		<!-- "V ~V PR N" => "V PR N ~V" -->
		<rule comment="REGLA: V V PR N">
			<pattern>
				<pattern-item n="SV"/>
				<pattern-item n="SV"/>
				<pattern-item n="PR"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_ND_nbr">  <!-- ND to "nbr" -->
					<with-param pos="1"/>
					<with-param pos="4"/>  <!-- "nbr" from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "can I have (...)" => "kann ich (...) haben" -->
		<!-- "Vx N ~V N" => "Vx N N ~V" -->
		<rule comment="REGLA: Vx NP V N">
			<pattern>
				<pattern-item n="SV_aux"/>
				<pattern-item n="SN_prn"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_ND_nbr">  <!-- ND to "nbr" -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- "nbr" from noun -->
				</call-macro>
				<out>
					<chunk>
						<clip pos="1" part="lem"/>
						<clip pos="1" part="tags"/>
						<clip pos="1" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="3" part="lem"/>
						<clip pos="3" part="tags"/>
						<clip pos="3" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


		<!-- "are you drinking coffee" => "trinkst du (gerade) Kaffee" -->
		<!-- "-Vs N ~V N" => "~V N N" -->
		<rule comment="REGLA: Vs NP V N">
			<pattern>
				<pattern-item n="SV_ser"/>
				<pattern-item n="SN_prn"/>
				<pattern-item n="SV"/>
				<pattern-item n="SN"/>
			</pattern>
			<action>
				<call-macro n="upd_ND_nbr">  <!-- ND to "nbr" -->
					<with-param pos="1"/>
					<with-param pos="2"/>  <!-- "nbr" from noun -->
				</call-macro>
				<out>
					<chunk>
						<get-case-from pos="1">
							<clip pos="3" part="lem"/>
						</get-case-from>
						<clip pos="1" part="tags"/>  <!-- tense from first verb -->
						<clip pos="3" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="2" part="lem"/>
						<clip pos="2" part="tags"/>
						<clip pos="2" part="chcontent"/>
					</chunk>
					<b/>
					<chunk>
						<clip pos="4" part="lem"/>
						<clip pos="4" part="tags"/>
						<clip pos="4" part="chcontent"/>
					</chunk>
				</out>
			</action>
		</rule>


