			<cat-item tags="CNJ.*"/>
		</def-cat>
		<def-cat n="SV">
			<cat-item tags="SV"/>
			<cat-item tags="SV.*"/>
			<cat-item tags="SV_aux"/>
			<cat-item tags="SV_aux.*"/>
			<cat-item tags="SV_ser"/>
			<cat-item tags="SV_ser.*"/>
			<cat-item tags="SV_haver"/>
			<cat-item tags="SV_haver.*"/>
			<cat-item tags="SV_do"/>
			<cat-item tags="SV_do.*"/>
		</def-cat>
		<def-cat n="SV_aux">
			<cat-item tags="SV_aux"/>
			<cat-item tags="SV_aux.*"/>
		</def-cat>
		<def-cat n="SV_ser">
			<cat-item tags="SV_ser"/>
			<cat-item tags="SV_ser.*"/>
		</def-cat>
		<def-cat n="SV_haver">
			<cat-item tags="SV_haver"/>
			<cat-item tags="SV_haver.*"/>
		</def-cat>
		<def-cat n="SV_do">
