<!-- macros: 	(by wth)
	set_		store tag into variable (set_[name] stores value in variable v_[name])
	chg_		change value of variable
	rm_         remove tag
	proc_       set variables for multiple words
-->

	<section-def-macros>

		<def-macro n="firstWord" npar="1">
			<let><var n="caseFirstWord"/><lit v="aa"/></let>
			<choose>
				<when>
					<test>
						<equal>
							<var n="EOS"/>
							<lit v="true"/>
						</equal>
					</test>
					<let>
						<var n="v_firstWord"/>
						<clip pos="1" side="tl" part="a_verb"/>
					</let>
				</when>
			</choose>
			<let>
				<var n="EOS"/>
				<lit v="false"/>
			</let>
		</def-macro>


		<!-- (verb) convert "pri" to "pres", "pii" to "past", "pis" to "past", "prs" to "inf" / OUT: v_temps -->
		<def-macro n="set_temps" npar="1">
			<choose>
				<when>
					<test>
						<equal>
							<clip pos="1" side="tl" part="a_temps"/>
							<lit-tag v="pri"/>
						</equal>
					</test>
					<let>
						<var n="v_temps"/>
						<lit-tag v="pres"/>
					</let>
				</when>
				<when>
					<test>
						<equal>
							<clip pos="1" side="tl" part="a_temps"/>
							<lit-tag v="pii"/>
						</equal>
					</test>
					<let>
						<var n="v_temps"/>
						<lit-tag v="past"/>
					</let>
				</when>
				<when>
					<test>
						<equal>
							<clip pos="1" side="tl" part="a_temps"/>
							<lit-tag v="pis"/>
						</equal>
					</test>
					<let>
						<var n="v_temps"/>
						<lit-tag v="past"/>
					</let>
				</when>
				<when>
					<test>
						<equal>
							<clip pos="1" side="tl" part="a_temps"/>
							<lit-tag v="prs"/>
						</equal>
					</test>
					<let>
						<var n="v_temps"/>
